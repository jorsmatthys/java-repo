<?xml version="1.0" encoding="UTF-8"?>
<project name="JGit Flow Maven Plugin"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd"
         xmlns="http://maven.apache.org/DECORATION/1.3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bannerLeft>
        <!-- Reflow Maven Skin, but with "Reflow" highlighted -->
        <name><![CDATA[
              <span class="color-highlight">JGit Flow</span> Maven Plugin
      
             ]]></name>
        <href>index.html</href>
    </bannerLeft>

    <publishDate position="navigation-bottom" format="yyyy-MM-dd" />
    <version position="navigation-bottom" />
    <googleAnalyticsAccountId>UA-63000101-1</googleAnalyticsAccountId>
    
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>
    <custom>
        <reflowSkin>
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>false</smoothScroll>
            <theme>default</theme>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>default</highlightJsTheme>
            <imgLightbox>false</imgLightbox>
            <!--<absoluteResourceURL>http://jgitflow.bitbucket.org</absoluteResourceURL>-->
            <brand>
                <!-- Reflow Maven Skin, but with "Reflow" highlighted -->
                <name><![CDATA[
                
                         <span class="color-highlight">JGit Flow</span> Maven Plugin
      
                      ]]></name>
                <href>index.html</href>
            </brand>
            <slogan>A maven plugin that allows you to use Git Flow and Maven Together!</slogan>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <!-- Use Table of Contents at the top of the page (max 6 elements) -->
            <toc>top</toc>
            <tocTopMax>6</tocTopMax>
            <!-- Include the documentation and tools in the top navigation (in addition to links) -->
            <topNav>Documentation|Contribute</topNav>

            <bottomNav maxSpan="9" >
                <column>Main</column>
                <column>Documentation</column>
                <column>Contribute</column>
            </bottomNav>

            <bottomDescription quote="false"></bottomDescription>
            <pages>
                <index project="jgitflow-maven-plugin">
                    <shortTitle>JGit Flow</shortTitle>
                    <!-- no breadcrumbs on the main index page -->
                    <breadcrumbs>false</breadcrumbs>
                    <!-- no ToC on the main index page -->
                    <toc>false</toc>
                    <sections>
                        <columns>2</columns>
                        <body/>
                    </sections>
                </index>
                <config>
                    <shortTitle>Configuration</shortTitle>
                </config>
            </pages>
        </reflowSkin>
    </custom>

    <body>
        <links>
            <item name="Bitbucket project" href="https://bitbucket.org/atlassian/jgit-flow/"/>
        </links>
        <breadcrumbs>
            <item name="JGit Flow Maven Plugin" href="/index.html"/>
        </breadcrumbs>
        <menu name="Main" inherit="top">
            <item name="Home" href="./"/>
            <item name="Bitbucket project" href="https://bitbucket.org/atlassian/jgit-flow/"/>
            <item name="Release Notes" href="release-notes.html"/>
        </menu>
        <menu name="Documentation" inherit="bottom">
            <item name="Mojo Documentation" href="plugin-info.html"/>
            <item name="Configuration" href="config.html"/>
            
        </menu>
        <menu name="Contribute" inherit="bottom">
            <!--<item name="Contribute" href="contribute.html"/>-->
            <item name="Issues" href="https://ecosystem.atlassian.net/browse/MJF"/>
            <item name="Fork on Bitbucket" href="https://bitbucket.org/atlassian/jgit-flow/"/>
            <item name="Google Group" href="https://groups.google.com/forum/#!forum/maven-jgitflow-users" />
            <item name="License" href="http://www.apache.org/licenses/LICENSE-2.0"/>
        </menu>
    </body>

</project>